<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.nuaa.entiy"><!-- 这里的package记得要写对啊。要不然运行也会出错的 -->
    <class name="Parameter" table="parameter"><!-- 如果表名跟类名一个样的话，那么table属性可以不写 -->
        <id name="Number"></id>
        <property name="Name"></property>
        <property name="Cname"></property>
        <property name="Length"></property>
        <property name="Opration"></property>
        <property name="Sort"></property>
        <property name="Sort1"></property>
        <property name="RangeFrom"></property>
        <property name="RangeTo"></property>
        <property name="Unit"></property>
        <property name="DataType"></property>
    </class>
</hibernate-mapping>